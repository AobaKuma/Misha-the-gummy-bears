<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef Name="Misha_ApparelBase" Abstract="True">
		<thingClass>Apparel</thingClass>
		<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<selectable>True</selectable>
		<pathCost>14</pathCost>
		<useHitPoints>True</useHitPoints>
		<techLevel>Industrial</techLevel>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
		</graphicData>
		<drawGUIOverlay>true</drawGUIOverlay>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<Beauty>-3</Beauty>
		</statBases>
		<tradeTags>
			<li>Clothing</li>
		</tradeTags>
		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<tickerType>Never</tickerType>
		<burnableByRecipe>true</burnableByRecipe>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="CompProperties_Styleable" />
			<li Class="CompProperties_Forbiddable" />
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<apparel>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
			<canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
			<tags>
				<li>Misha_Civilian</li>
			</tags>
		</apparel>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>HandTailoringBench</li>
				<li>ElectricTailoringBench</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
	</ThingDef>

	<!-- Onskin -->
	<ThingDef  ParentName="Misha_ApparelBase" Name="Misha_ApparelOnSkinBase" Abstract="True">
		<apparel>
			<layers>
				<li>OnSkin</li>
			</layers>
		</apparel>
	</ThingDef>

	<ThingDef ParentName="Misha_ApparelOnSkinBase">
		<defName>Misha_Apparel_Onskin_Airf</defName>
		<label>onskin Airf</label>
		<description>佔位符</description>
		<graphicData>
			<texPath>Misha_Apparel/Onskin/Airf/icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Steel>20</Steel>
		</costList>
		<costStuffCount>75</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>160</MaxHitPoints>
			<MarketValue>200</MarketValue>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<ArmorRating_Sharp>0.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
			<StuffEffectMultiplierArmor>0.20</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.36</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.40</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<wornGraphicPath>Misha_Apparel/Onskin/Airf/apparel</wornGraphicPath>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<tags>
				<li>Misha_Civilian</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>

