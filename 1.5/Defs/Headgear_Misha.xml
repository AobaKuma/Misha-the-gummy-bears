	<ThingDef ParentName="Misha_ApparelOverheadBase">
		<defName>Misha_Apparel_Overhead_Airf</defName>
		<label>headgear Airf</label>
		<description>佔位符</description>
		<graphicData>
			<texPath>Misha_Headgear/Airf/icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Steel>20</Steel>
		</costList>
		<costStuffCount>75</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>160</MaxHitPoints>
			<MarketValue>200</MarketValue>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<ArmorRating_Sharp>0.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
			<StuffEffectMultiplierArmor>0.20</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.36</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.40</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<wornGraphicPath>Misha_Headgear/Airf/headgear</wornGraphicPath>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<tags>
				<li>Misha_Civilian</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>

